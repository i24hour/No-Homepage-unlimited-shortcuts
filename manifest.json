{
    "manifest_version": 3,
    "name": "iCTrL - Tab Control & Quick Links",
    "version": "1.1",
    "description": "Control your browser! Quick link shortcuts on new tab + RAM Saver that auto-closes inactive tabs.",
    "permissions": [
        "storage",
        "tabs",
        "alarms",
        "identity"
    ],
    "oauth2": {
        "client_id": "YOUR_OAUTH_CLIENT_ID.apps.googleusercontent.com",
        "scopes": [
            "openid",
            "email",
            "profile",
            "https://www.googleapis.com/auth/cloud-platform"
        ]
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "iCTrL - Control Your Browser"
    },
    "commands": {
        "open-full-page": {
            "suggested_key": {
                "default": "Ctrl+Shift+L",
                "mac": "Command+Shift+L"
            },
            "description": "Open Quick Links full page"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    }
}